<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.1//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_2.dtd'>
<nta>
	<declaration>clock CTimer;
int ErrStat = 0;
chan OpenClutch, ClutchIsOpen;</declaration>
	<template>
		<name>Interface</name>
		<location id="id0" x="0" y="0">
		</location>
		<init ref="id0"/>
	</template>
	<template>
		<name>GearBox</name>
		<location id="id1" x="0" y="0">
		</location>
		<init ref="id1"/>
	</template>
	<template>
		<name x="5" y="5">Clutch</name>
		<location id="id2" x="-161" y="8">
			<name x="-195" y="25">Closed</name>
		</location>
		<location id="id3" x="-161" y="-161">
			<name x="-196" y="-212">Opening</name>
			<label kind="invariant" x="-212" y="-195">CTimer &lt;= 150</label>
		</location>
		<location id="id4" x="144" y="-161">
			<name x="134" y="-195">Open</name>
		</location>
		<location id="id5" x="144" y="8">
			<name x="119" y="25">Closing</name>
			<label kind="invariant" x="102" y="42">CTimer &lt;= 150</label>
		</location>
		<location id="id6" x="42" y="-76">
			<name x="8" y="-110">ErrorClose</name>
		</location>
		<location id="id7" x="-59" y="-76">
			<name x="-93" y="-59">ErrorOpen</name>
		</location>
		<init ref="id2"/>
		<transition>
			<source ref="id3"/>
			<target ref="id4"/>
			<label kind="guard" x="-68" y="-195">CTimer &gt;= 100</label>
			<label kind="synchronisation" x="-59" y="-178">ClutchIsOpen!</label>
		</transition>
		<transition>
			<source ref="id2"/>
			<target ref="id3"/>
			<label kind="synchronisation" x="-255" y="-59">OpenClutch?</label>
			<label kind="assignment" x="-255" y="-76">CTimer := 0</label>
		</transition>
		<transition>
			<source ref="id3"/>
			<target ref="id7"/>
			<label kind="guard" x="-144" y="-136">CTimer == 150</label>
			<label kind="assignment" x="-127" y="-119">ErrStat := 2</label>
		</transition>
		<transition>
			<source ref="id5"/>
			<target ref="id6"/>
			<label kind="guard" x="25" y="-51">CTimer == 150</label>
			<label kind="assignment" x="51" y="-34">ErrStat := 1</label>
		</transition>
		<transition>
			<source ref="id4"/>
			<target ref="id5"/>
			<label kind="synchronisation" x="144" y="-93">OpenClutch!</label>
			<label kind="assignment" x="144" y="-76">CTimer := 0</label>
		</transition>
		<transition>
			<source ref="id5"/>
			<target ref="id2"/>
			<label kind="guard" x="-59" y="8">CTimer &gt;= 100</label>
			<label kind="synchronisation" x="-51" y="25">ClutchIsOpen?</label>
		</transition>
	</template>
	<template>
		<name>Engine</name>
		<location id="id8" x="0" y="0">
		</location>
		<init ref="id8"/>
	</template>
	<template>
		<name>GearControl</name>
		<location id="id9" x="0" y="0">
		</location>
		<init ref="id9"/>
	</template>
	<system>// Place template instantiations here.
Process = Clutch();
// List one or more processes to be composed into a system.
system Process;
    </system>
	<queries>
		<query>
			<formula></formula>
			<comment></comment>
		</query>
	</queries>
</nta>
